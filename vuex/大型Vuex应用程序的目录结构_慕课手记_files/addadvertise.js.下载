define(function(require){var a=require("//coding.imooc.com/static/lib/adv/insetadvertising.js");$.get("/common/adver-getadver",function(g){if(g=JSON.parse(g),0==g.result){var c=g.data.globalRightFloat,v=g.data.globalTopBanner;c.length>0&&(c=c[0],a.initAdv("#moco-right-float-advertising",{position:"right",image:"//img.mukewang.com/"+c.pic+"-160-160.jpg",link:c.links,advid:c.id,animate:"fade"})),v.length>0&&(v=v[0],a.initAdv("#moco-top-advertising",{position:"top",image:"//img.mukewang.com/"+v.pic+".jpg",link:v.links,advid:v.id,animate:"slide"}))}})});