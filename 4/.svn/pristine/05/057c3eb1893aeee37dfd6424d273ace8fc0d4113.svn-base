package com.ck.springboot.dto;

import java.io.Serializable;

import lombok.Data;

@Data
public class MedcineDTO implements Serializable
{
	private static final long serialVersionUID = -7170851535017844166L;
	private String commonName; // 药品通用名
	private String specification; // 规格
	private double eachDose; // 每次剂量
	private String eachDoseUnit; // 每次剂量单位
	private double total; // 总量
	private String totalUnit; // 总量单位
	private String routeOfMedication; // 给药途径
	private String frequency; // 给药频率
	private double price; // 药品单价
	private int duration; // 给药持续时间，单位每天
	private int dailyDose; // 每日服用次数
}
