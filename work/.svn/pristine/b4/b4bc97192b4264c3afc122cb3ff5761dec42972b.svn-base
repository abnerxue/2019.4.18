package ck.po;

import java.io.Serializable;

import lombok.Data;

/**
 * 
 * 
 * @author alex
 * 
 */

@Data
public class Person implements Serializable
{
	private static final long serialVersionUID = -3562550857760039655L;
	private String name;
	private int age;

	public Person(String name, int age)
	{
		super();
		this.name = name;
		this.age = age;
	}

}